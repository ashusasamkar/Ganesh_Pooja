<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment_list2">

    <!-- TODO: Update blank fragment layout -->

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/list4"/>
    <ImageButton
        android:id="@+id/next_2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:onClick="next"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:background="@drawable/ic_navigate_next_black_24dp"/>

    <ImageButton
        android:id="@+id/back_2"
        android:layout_width="50dp"
        android:onClick="back"
        android:layout_height="50dp"
        android:layout_centerVertical="true"
        android:background="@drawable/ic_keyboard_arrow_left_black"/>


</RelativeLayout>